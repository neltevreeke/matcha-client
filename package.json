{
  "name": "matcha-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run dll && NODE_ENV=development webpack-dev-server --config ./scripts/webpack/webpackConfig.js",
    "dist": "npm run clean:dist && npm run dist:webpack && npm run dist:copy",
    "dist:webpack": "NODE_ENV=production webpack --display-error-details --verbose --bail --env.NODE_ENV=production --env.production --progress --config ./scripts/webpack/webpackConfig.js",
    "dist:copy": "cpy public ../dist --cwd=./src/ --parents && cpy ./src/_redirects ./dist",
    "dll": "NODE_ENV=development node ./scripts/dll.js",
    "lint": "run-p lint:*",
    "lint:js": "eslint . --fix",
    "dist-server": "npm run dist && NODE_ENV=production node ./scripts/distServer.js",
    "clean:dist": "rm -rf ./dist",
    "clean:node_modules": "rm -rf ./node_modules",
    "clean:package-lock": "rm -rf ./package-lock.json",
    "wipe": "run-s clean:coverage clean:dist clean:package-lock clean:node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "dll": {
    "exclude": [
      "sanitize.css"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
